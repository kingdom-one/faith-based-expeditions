{
	"version": 3,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"background": true,
			"customDuotone": false,
			"customGradient": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"slug": "green",
					"color": "#18312e",
					"name": "Green"
				},
				{
					"slug": "green-2",
					"color": "#374836",
					"name": "Green 2"
				},
				{
					"slug": "gold",
					"color": "#f9d790",
					"name": "Gold"
				},
				{
					"slug": "white",
					"color": "#fff",
					"name": "White"
				},
				{
					"slug": "black",
					"color": "#000",
					"name": "Black"
				}
			],
			"text": true,
			"heading": true,
			"button": true
		},
		"dimensions": {
			"aspectRatio": true,
			"minHeight": true
		},
		"layout": {
			"contentSize": "640px",
			"wideSize": "1200px"
		},
		"spacing": {
			"blockGap": true,
			"customSpacingSize": true,
			"margin": true,
			"padding": true,
			"units": [ "rem", "%" ]
		},
		"typography": {
			"customFontSize": true,
			"defaultFontSizes": false,
			"lineHeight": true,
			"dropCap": false,
			"fluid": true,
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"textDecoration": true,
			"textTransform": true,
			"fontSizes": [
				{
					"slug": "xs",
					"size": "clamp(0.8rem, 0.941176vi + 0.56rem, 1.2rem)",
					"name": "XS"
				},
				{
					"slug": "base",
					"size": "clamp(1rem, 1.176471vi + 0.71rem, 1.5rem)",
					"name": "Base"
				},
				{
					"slug": "small",
					"size": "clamp(1.25rem, 1.482353vi + 0.88rem, 1.88rem)",
					"name": "Small (H6)"
				},
				{
					"slug": "medium",
					"size": "clamp(1.56rem, 1.835294vi + 1.1rem, 2.34rem)",
					"name": "Medium (H5)"
				},
				{
					"slug": "large",
					"size": "clamp(1.95rem, 2.305882vi + 1.37rem, 2.93rem)",
					"name": "Large (H4)"
				},
				{
					"slug": "xl",
					"size": "clamp(2.44rem, 2.870588vi + 1.72rem, 3.66rem)",
					"name": "XL (H3)"
				},
				{
					"slug": "2xl",
					"size": "clamp(3.05rem, 3.6vi + 2.15rem, 4.58rem)",
					"name": "3XL (H2)"
				},
				{
					"slug": "3xl",
					"size": "clamp(3.81rem, 4.494118vi + 2.69rem, 5.72rem)",
					"name": "3XL (H1)"
				}
			],
			"fontFamilies": [
				{
					"slug": "body",
					"name": "Roboto",
					"fontFamily": "'Roboto', sans-serif",
					"fontFace": [
						{
							"fontFamily": "Roboto",
							"fontStyle": "normal",
							"fontWeight": 300,
							"src": [ "file:./fonts/Roboto/Roboto-Light.ttf" ]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "italic",
							"fontWeight": 300,
							"src": [
								"file:./fonts/Roboto/Roboto-LightItalic.ttf"
							]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "normal",
							"fontWeight": 400,
							"src": [ "file:./fonts/Roboto/Roboto-Regular.ttf" ]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "italic",
							"fontWeight": 400,
							"src": [ "file:./fonts/Roboto/Roboto-Italic.ttf" ]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "normal",
							"fontWeight": 700,
							"src": [ "file:./fonts/Roboto/Roboto-Bold.ttf" ]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "italic",
							"fontWeight": 700,
							"src": [
								"file:./fonts/Roboto/Roboto-BoldItalic.ttf"
							]
						}
					]
				},
				{
					"slug": "headings",
					"name": "Cormorant",
					"fontFamily": "'Cormorant', serif",
					"fontFace": [
						{
							"fontFamily": "Cormorant",
							"fontStyle": "normal",
							"fontWeight": "300 900",
							"src": [
								"file:./fonts/Cormorant/Cormorant-VariableFont_wght.ttf"
							]
						},
						{
							"fontFamily": "Cormorant",
							"fontStyle": "italic",
							"fontWeight": "300 900",
							"src": [
								"file:./fonts/Cormorant/Cormorant-Italic-VariableFont_wght.ttf"
							]
						}
					]
				}
			]
		},
		"custom": {
			"lineHeight": "calc(4px + 2.5ex)",
			"transitionTiming": ".15s",
			"transitionEasing": "ease-in-out",
			"transition": "var:custom|transition-timing var:custom|transition-easing",
			"gutters": "1.25rem"
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "0"
		},
		"typography": {
			"fontSize": "var:preset|font-size|base",
			"lineHeight": "var:custom|line-height"
		},
		"blocks": {
			"core/paragraph": {
				"typography": {
					"fontSize": "var:preset|font-size|base",
					"lineHeight": "var:custom|line-height",
					"fontFamily": "var:preset|font-family|body"
				}
			},
			"core/list": {
				"typography": {
					"fontSize": "var:preset|font-size|base",
					"lineHeight": "var:custom|line-height",
					"fontFamily": "var:preset|font-family|body"
				}
			},
			"core/list-item": {
				"typography": {
					"fontSize": "var:preset|font-size|base",
					"lineHeight": "var:custom|line-height",
					"fontFamily": "var:preset|font-family|body"
				}
			},
			"core/heading": {
				"typography": {
					"lineHeight": "var:custom|line-height",
					"fontFamily": "var:preset|font-family|headings"
				}
			}
		},
		"elements": {
			"heading": {
				"typography": {
					"lineHeight": "var:custom|line-height",
					"fontFamily": "var:preset|font-family|headings"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|3-xl"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|2-xl"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|xl"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|large"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var:preset|font-size|medium"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var:preset|font-size|small"
				}
			}
		}
	}
}
